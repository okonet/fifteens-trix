{
  "stage": 0,
  "plugins": [
    "react-transform",
    "typecheck"
  ],
  "extra": {
    "react-transform": {
      "transforms": [
        {
          "transform": "react-transform-hmr",
          "imports": ["react"],
          "locals": ["module"]
        }, {
          "transform": "react-transform-catch-errors",
          "imports": ["react", "redbox-react"]
        }
      ]}
  }
}
